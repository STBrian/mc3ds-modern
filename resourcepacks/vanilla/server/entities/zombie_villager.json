                    "baby",
                    "jockey"
                  ]

                }
              }
            ]
          },
          {
            "filters": { "without_components": "minecraft:variant" },
            "randomize": [
              {
                "weight": 5,
                "add": { "component_groups": [ "farmer" ] }
              },
              {
                "weight": 5,
                "add": { "component_groups": [ "fisherman" ] }
              },
              {
                "weight": 5,
                "add": { "component_groups": [ "shepherd" ] }
